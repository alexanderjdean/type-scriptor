{
  "name": "type-scriptor",
  "version": "1.0.6",
  "description": "An NPM package that converts your JavaScript code to TypeScript code, adds type annotations, clear documentation, and unit tests",
  "license": "MIT",
  "repository": "https://github.com/alexanderjdean/type-scriptor.git",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "bin": {
    "type-scriptor": "dist/cli.js"
  },
  "keywords": [
    "typescript",
    "js-to-ts",
    "gpt4",
    "unit-tests",
    "generate-documentation"
  ],
  "author": "Alexander Dean",
  "devDependencies": {
    "@types/command-line-args": "^5.2.0",
    "@types/minimist": "^1.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vitest": "^0.29.8"
  },
  "dependencies": {
    "chalk": "^4",
    "command-line-args": "^5.2.1",
    "dotenv": "^16.0.3",
    "recast": "^0.22.0"
  }
}
